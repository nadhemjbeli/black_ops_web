<form method="post" action="{{ path('app_admin_skin_delete', {'idSkin': skin.idSkin}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ skin.idSkin) }}">
    <button class="btn btn-outline-success">Delete</button>
</form>
